    "version": "0.2.0",
    "configurations": [
        {
            "name": "splash_example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "splash_example (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "splash_example (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "splash_example (dev)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_apple.dart",
            "args": [
                "--flavor",
                "apple"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "splash_example (prod)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_banana.dart",
            "args": [
                "--flavor",
                "banana"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}